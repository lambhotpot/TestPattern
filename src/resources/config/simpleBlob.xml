<?xml version="1.0"?>
<!--The following parameter is applicable to the resized image of 100 * 100 size assuming the border is already trimmed-->
<opencv_storage>
    <format>3</format>
    <thresholdStep>10</thresholdStep>
    <minThreshold>10</minThreshold>
    <maxThreshold>250</maxThreshold>
    <minRepeatability>2</minRepeatability>
    <minDistBetweenBlobs>5</minDistBetweenBlobs>
    <filterByColor>0</filterByColor>
    <blobColor>0</blobColor>
    <filterByArea>1</filterByArea>
    <minArea>500</minArea>
    <maxArea>5000.</maxArea>
    <filterByCircularity>1</filterByCircularity>
    <minCircularity>0</minCircularity>
    <maxCircularity>1</maxCircularity>
    <filterByInertia>1</filterByInertia>
    <minInertiaRatio>0</minInertiaRatio>
    <maxInertiaRatio>1</maxInertiaRatio>
    <filterByConvexity>1</filterByConvexity>
    <minConvexity>0</minConvexity>
    <maxConvexity>1</maxConvexity>
</opencv_storage>
